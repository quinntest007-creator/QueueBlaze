# Environment Variables for QueueBlaze
# Copy this to .env and fill in your values

# Django Secret Key (change this in production!)
DJANGO_SECRET_KEY=your-secret-key-here

# Debug mode (False in production!)
DEBUG=False

# Allowed hosts (comma-separated)
ALLOWED_HOSTS=your-domain.onrender.com,yourapp.onrender.com

# Database URL (from Render PostgreSQL)
# Format: postgres://username:password@host:5432/database_name
DATABASE_URL=postgres://your-db-url-here
